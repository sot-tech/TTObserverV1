{
	"log": {
		"file": "tt.log",
		"level": "DEBUG"
	},
	"crawler": {
		"url": {
			"base": "http://site.local",
			"torrent": "/content/torrent/%d",
			"extractnameactions": [
				{
					"action": "go",
					"param": "/catalog"
				},
				{
					"action": "extract",
					"param": "<p class=\"catalog_element\">.*?<a .*?href=\"(.*?)\".*?>"
				},
				{
					"action": "go",
					"param": "${arg}"
				},
				{
					"action": "check",
					"param": "<div class=\"release_torrent\">.*?<a class=\"button button_black\" href=\"\\Q${torrent}\\E\">"
				},
				{
					"action": "extract",
					"param": "<div class=\"main_title\">.*?<span>(.*?)</span>"
				},
				{
					"action": "return",
					"param": ""
				}
			],
			"extractimageactions": [
				{
					"action": "go",
					"param": "/catalog"
				},
				{
					"action": "extract",
					"param": "<p class=\"catalog_element\">.*?<a .*?href=\"(.*?)\".*?>"
				},
				{
					"action": "go",
					"param": "${arg}"
				},
				{
					"action": "check",
					"param": "<div class=\"release_torrent\">.*?<a class=\"button button_black\" href=\"\\Q${torrent}\\E\">"
				},
				{
					"action": "extract",
					"param": "<div id=\"release_main_poster\" style=\"background-image: url\\((.*?)\\)\".*?>"
				},
				{
					"action": "go",
					"param": "${arg}"
				},
				{
					"action": "return",
					"param": ""
				}
			]
		},
		"threshold": 10,
		"errorthreshold": 10,
		"delay": 10
	},
	"tryextractprettyname": true,
	"tryextractimage": true,
	"sizethreshold": 5.0,
	"telegramtoken": "123456:1234567890abcdef",
	"adminotpseed": "BASE32LONGLONGSEEDSTRING",
	"dbfile": "conf/example.db",
	"msg": {
		"announce": "*Torrent ${action}*\n`${name}:${size}`\n[Download](${url})\n",
		"n1000": "New GET ${index}",
		"added": "added",
		"updated": "updated",
		"error": "Operation error: ${msg}",
		"replacements": {
			"_": " "
		},
		"cmds": {
			"start": "Available commands:\n/start - this help\n/attach - subscribe\n/detach - unsubscribe\n/state - state of chat",
			"attach": "Added to announce list",
			"detach": "Removed from announce list",
			"setadmin": "You now admin",
			"rmadmin": "You now not admin",
			"state": "Watch: ${watch}\nAdmin: ${admin}\nNext index: ${index}",
			"auth": "Unauthorized",
			"unknown": "Unknown command"
		}
	}	
}
