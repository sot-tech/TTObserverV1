{
	"log": {
		"file": "/var/log/tto.log",
		"level": "WARNING"
	},
	"crawler": {
		"baseurl": "http://localhost.localdomain",
		"contexturl": "/content/torrent/%d",
		"threshold": 10,
		"delay": 10,
		"anniversary": 100,
		"metaactions": [
			{
				"action": "go",
				"param": "/catalog"
			},
			{
				"action": "extract",
				"param": "<p class=\"catalog_info_name\">.*?<a .*?href=\"(?P<url>.*?)\".*?>"
			},
			{
				"action": "store",
				"param": ""
			},
			{
				"action": "go",
				"param": "${arg}"
			},
			{
				"action": "findFirst",
				"param": "<div class=\"release_torrent_butt\">.*?<a class=\"button button_black\" href=\"\\Q${search}\\E\">"
			},
			{
				"action": "extract",
				"param": "<div id=\"release_main_data[_a-zA-Z]*?\">[\\s\\r\\n]*?<div class=\"release_reln\">[\\s\\r\\n]*?<span>[\\s\\r\\n]*?(?P<name_en>.*?)[\\s\\r\\n]*?<\\/span>[\\s\\r\\n]*?<\\/div>|<div id=\"release_main_poster\" style=\"background-image: url\\((?P<poster>.*?)\\)\".*?>"
			},
			{
				"action": "store",
				"param": ""
			}
		],
		"imagemetafield": "poster",
		"imagethumb": 1280
	},
	"telegram": {
		"apiid": 123456,
		"apihash":"kqwrvonebe34283on3on",
		"bottoken": "123321:BOIBGvOUVUYVYBGIoiboiubib",
		"dbpath": "/var/lib/tto/tgdb",
		"filestorepath": "/tmp/tgf",
		"otpseed": "SOMERANDOMBASE32LONGSTRING"
	},
	"msg": {
		"announce": "**Torrent {{.action}}**\nName: `{{.meta.name_en}} (File name: {{.name}})`\nSize: `{{.size}}`\nFile count: `{{.filecount}}`\n[DownloadðŸ“¥]({{.url}})\n",
